public class TestPolymorphisim {
		public static void main(String[] args) {
			Person persons[]=new Person[3];
			persons[0] = new Student("Adam","Walter",2021,8.5);
			persons[1] = new Student("Bill","Gates",2022,9.5);
			persons[2] = new Professor("Soma","Gupta");
			
			for(Person person : persons) {
				person.print();
			}
			
		}
}



public class TestOverride {
	public static void main(String[] args) {
		Professor knowIt=new Professor("Soma","Gupta");
		knowIt.print();
	}
}
